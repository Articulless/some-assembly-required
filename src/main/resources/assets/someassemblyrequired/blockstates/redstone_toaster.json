        "model": "someassemblyrequired:block/idle_redstone_toaster",
        "y": 0
      }
    },
    {
      "when": {
        "toasting": false,
        "facing": "east"
      },
      "apply": {
        "model": "someassemblyrequired:block/idle_redstone_toaster",
